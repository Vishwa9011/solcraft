{
   "plugins": ["prettier-plugin-tailwindcss"],
   "tailwindFunctions": ["clsx"],
   "trailingComma": "es5",
   "tailwindPreserveWhitespace": true,
   "tabWidth": 3,
   "useTabs": false,
   "printWidth": 120,
   "semi": true,
   "singleQuote": true,
   "bracketSpacing": true,
   "jsxBracketSameLine": false,
   "arrowParens": "avoid",
   "htmlWhitespaceSensitivity": "strict",
   "singleAttributePerLine": false
}
